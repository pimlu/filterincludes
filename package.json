{
  "name": "filterincludes",
  "displayName": "Filter Includes",
  "publisher": "pimlu",
  "description": "Filters unused includes from your source code",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/pimlu/filterincludes.git"
  },
  "version": "1.0.1",
  "license": "GPL-3.0",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.filterIncludes"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [{
      "command": "extension.filterIncludes",
      "title": "Filter Includes"
    }]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
